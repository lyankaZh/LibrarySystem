<Window x:Class="Library.AddWindows.AddNewPublisherWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library"
        mc:Ignorable="d"
        Title="AddNewPublisher" Height="244.186" Width="345.349">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="publisherNameLabel" Content="Publisher Name" HorizontalAlignment="Left" Margin="30,31,0,0" VerticalAlignment="Top" Width="120" Height="29"/>
        <TextBox x:Name="publisherNameTextBox" HorizontalAlignment="Left" Height="23" Margin="187,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.308,-0.719"/>
        <Label x:Name="cityLabel" Content="City" HorizontalAlignment="Left" Margin="30,60,0,0" VerticalAlignment="Top" Width="120" Height="29"/>
        <Label x:Name="countyLabel" Content="Country" HorizontalAlignment="Left" Margin="30,93,0,0" VerticalAlignment="Top" Width="120" Height="29"/>
        <TextBox x:Name="cityTextBox" HorizontalAlignment="Left" Height="23" Margin="187,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.308,-0.719"/>
        <TextBox x:Name="countryTextBox" HorizontalAlignment="Left" Height="23" Margin="187,99,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.308,-0.719"/>
        <Button x:Name="saveButton" Content="Save" HorizontalAlignment="Left" Margin="62,172,0,0" VerticalAlignment="Top" Width="75" Height="20" Click="saveButton_Click"/>
        <Button x:Name="cancelButtonCopy" Content="Cancel" HorizontalAlignment="Left" Margin="187,172,0,0" VerticalAlignment="Top" Width="75" Height="20" Click="cancelButtonCopy_Click"/>

    </Grid>
</Window>
